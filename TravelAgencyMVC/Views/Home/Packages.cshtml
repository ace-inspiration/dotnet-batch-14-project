@model List<TravelPackage>



<section class="parallax-window" data-parallax="scroll" data-image-src="../img/slides/beach.jpg" data-natural-width="1400" data-natural-height="470">
	<div class="parallax-content-1 opacity-mask" data-opacity-mask="rgba(0, 0, 0, 0.6)">
		<div class="animated fadeInDown">
			<h1>Paris tours</h1>
			<p>Cursus neque cursus curae ante scelerisque vehicula</p>
		</div>
	</div>
</section>

<main>

	<div id="position">
		<div class="container">
			<ul>
				<li>
					<a href="#">Home</a>
				</li>
				<li>
					<a href="#">Category</a>
				</li>
				<li>Page active</li>
			</ul>
		</div>
	</div>
	<!-- Position -->

	<div class="collapse" id="collapseMap">
		<div id="map" class="map"></div>
	</div>
	<!-- End Map -->


	<div class="container margin_60">

		<div class="row">

			<div class="col text-center">

				<div id="tools">
					<div class="row justify-content-between">
						<div class="col-md-3 col-sm-4">
							<div class="styled-select-filters">
								<select name="sort_price" id="sort_price">
									<option value="" selected>Sort by price</option>
									<option value="lower">Lowest price</option>
									<option value="higher">Highest price</option>
								</select>
							</div>
						</div>
						

					</div>
				</div>
				<!--/tools -->
				@if(Model.Count != 0) {
						@foreach (var package in Model)
						{
							<div class="strip_all_tour_list wow fadeIn" data-wow-delay="0.1s">
								<div class="row">
									<div class="col-lg-4 col-md-4 position-relative">
										<div class="ribbon_3 popular">
											<span>@package.Status</span>
										</div>
										
										<div class="img_list">
											<a href="single_tour.html">
												<img src="@package.Image" alt="@package.Title">
												<div class="short_info"><i class="icon_set_1_icon-4"></i>@package.Destination</div>
											</a>
										</div>
									</div>
									<div class="col-lg-6 col-md-6">
										<div class="tour_list_desc">
											<div class="rating">
												<h2>Package Title</h2>
											</div>
											<h3><strong>@package.Title</strong> tour</h3>
											<p>@package.Description</p>
											<ul class="add_info">
												<li>
													<div class="tooltip_styled tooltip-effect-4">
														<span class="tooltip-item"><i class="icon_set_1_icon-83"></i></span>
														@* <div class="tooltip-content"> *@
														@* 	<h4>Duration</h4> *@
														@* 	<strong>@package.Duration <span>Days</span></strong> *@
														@* </div> *@
													</div>
												</li>
												<li>
													<div class="tooltip_styled tooltip-effect-4">
														<span class="tooltip-item"><i class="icon_set_1_icon-97"></i></span>
														<div class="tooltip-content">
															<h4>Inclusions</h4> @package.Inclusions
														</div>
													</div>
												</li>
											</ul>
										</div>
									</div>
									<div class="col-lg-2 col-md-2">
										<div class="price_list">
											<div>
											<span class="fs-3">@package.Price  MMK</span><small>*Per person</small>
												<p>
												<a href="/Home/PackageDetail/@package.Id" class="btn_1">Details</a>
												</p>
											</div>
										</div>
									</div>
								</div>
							</div>
						}
				<!--End strip -->



				@* <nav aria-label="Page navigation"> *@
				@* 	<ul class="pagination justify-content-center"> *@
				@* 		<li class="page-item"> *@
				@* 			<a class="page-link" href="#" aria-label="Previous"> *@
				@* 				<span aria-hidden="true">&laquo;</span> *@
				@* 				<span class="sr-only">Previous</span> *@
				@* 			</a> *@
				@* 		</li> *@
				@* 		<li class="page-item active"> *@
				@* 			<span class="page-link">1</span> *@
				@* 		</li> *@
				@* 		<li class="page-item"><a class="page-link" href="#">2</a></li> *@
				@* 		<li class="page-item"><a class="page-link" href="#">3</a></li> *@
				@* 		<li class="page-item"> *@
				@* 			<a class="page-link" href="#" aria-label="Next"> *@
				@* 				<span aria-hidden="true">&raquo;</span> *@
				@* 				<span class="sr-only">Next</span> *@
				@* 			</a> *@
				@* 		</li> *@
				@* 	</ul> *@
				@* </nav> *@
				}
				else{
					<div>
						<h2 class="text-center text-danger">No Data Found!</h2>
					</div>
				}
				<!-- end pagination-->

			</div>
			<!-- End col lg-9 -->
		</div>
		<!-- End row -->
	</div>
	<!-- End container -->
</main>
<!-- End main -->